# Production environment template for Hackathon Registration and Management System

# MongoDB Atlas
MONGODB_URI=mongodb+srv://<db_user>:<db_password>@<cluster>/<db_name>?retryWrites=true&w=majority&appName=Cluster0

# Security
JWT_SECRET=replace_with_a_long_random_secret_min_32_chars
JWT_EXPIRATION_MS=86400000
FACULTY_SECRET_CODE=KLHAZ
ADMIN_EMAIL=faculty@hackathon.com
ADMIN_PASSWORD=Admin@123

# Razorpay (required for live payments)
PAYMENT_MOCK_ENABLED=false
RAZORPAY_KEY_ID=rzp_live_xxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=xxxxxxxxxxxxxxxxxxxx

# Optional fee config
PAYMENT_FEE_SPLIT_MEMBERS=4
